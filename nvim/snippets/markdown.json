{
  "Add Link": {
    "prefix": "tl",
    "body": ["[$1]($2) ", "", "$3"],
    "description": "Text Link"
  },
  "Image Link": {
    "prefix": "il",
    "body": ["![$1]($2) ", "", "$3"],
    "description": "Image Link"
  },
  "Heading1": {
    "prefix": "h1",
    "body": ["# $1 ", "", "$2"],
    "description": "Heading 1"
  },
  "Heading2": {
    "prefix": "h2",
    "body": ["## $1 ", "", "$2"],
    "description": "Heading 2"
  },
  "Heading3": {
    "prefix": "h3",
    "body": ["### $1 ", "", "$2"],
    "description": "Heading 3"
  },
  "Heading4": {
    "prefix": "h4",
    "body": ["#### $1 ", "", "$2"],
    "description": "Heading 4"
  },
  "Heading5": {
    "prefix": "h5",
    "body": ["##### $1 ", "", "$2"],
    "description": "Heading 5"
  },
  "Code block": {
    "prefix": "cd",
    "body": ["```$1\n$2\n```\n\n$3"],
    "description": "Code block"
  },
  "Checkbox": {
    "prefix": "cb",
    "body": ["- [ ] $1 ", "$2"],
    "description": "Checkbox"
  },
  "Only Checkbox": {
    "prefix": "c",
    "body": ["[ ] $1 ", "$2"],
    "description": "Only Checkbox"
  },
  "Bold": {
    "prefix": "b",
    "body": ["**$1** $2"],
    "description": "Bold"
  },
  "Emphasis": {
    "prefix": "em",
    "body": ["`$1` $2"],
    "description": "Emphasis"
  },
  "Keyboard": {
    "prefix": "kd",
    "body": ["<kbd>$1</kbd> $2"],
    "description": "Keyboard"
  },
  "Comment": {
    "prefix": "cm",
    "body": ["<!--$0-->", "", "$1"],
    "description": "Comment"
  }
}
